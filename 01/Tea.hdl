
/**
 * Tea:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP Tea {
    IN sel[2], t, bi;
    OUT bl, m;

    PARTS:
	Or(a=t, b=bi, out=tbi);
	DMux4Way(in=tbi, a=a, b=b, c=c, d=d, sel[0]=sel[0], sel[1]=sel[1]);
	//Mux();
}