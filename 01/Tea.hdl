
/**
 * Tea:
 * 
 */

CHIP Tea {
    IN d[2], t, bi;
    OUT bl, m;

    PARTS:
	Not(in=d[0], out=notd1);
	Not(in=d[1], out=notd2);
	And(a=notd1, b=notd2, out=ada);
	And(a=d[0], b=notd2, out=george);
	Not(in=t, out=nott);
	And(a=notd1, b=d[1], out=milk);
	And(a=george, b=bi, out=bis);
	Or(a=milk, b=bis, out=m);
	And(a=ada, b=nott, out=bl);
	
}